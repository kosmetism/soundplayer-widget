{
  "name": "soundplayer-widget",
  "version": "0.1.4",
  "description": "Lightweight embedded SoundCloud player widgets",
  "main": "src/index.js",
  "style": "cssnext/index.css",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build-css": "cssnext cssnext/index.css dist/soundplayer-widget.min.css -c",
    "build": "NODE_ENV=production webpack --config webpack.config.js --progress --colors -p",
    "watch": "webpack --config webpack.config.js --progress --colors --watch",
    "prepublish": "npm run build"
  },
  "keywords": [
    "soundcloud",
    "widget",
    "player",
    "embed",
    "react",
    "deku",
    "functional",
    "component"
  ],
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "deku": "^0.4.5",
    "soundcloud-audio": "^0.1.4"
  },
  "devDependencies": {
    "babel": "^5.2.15",
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "cssnext": "^1.4.0",
    "cssnext-loader": "^1.0.1",
    "envify": "^3.4.0",
    "node-libs-browser": "^0.5.2",
    "style-loader": "^0.12.3",
    "transform-loader": "^0.2.2",
    "uglify-js": "^2.4.23",
    "webpack": "^1.9.10"
  }
}
